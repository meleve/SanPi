<h1>Movimiento de Caja</h1>

<table class="table table-striped table-hover table-condensed dt-responsive" id="datatable">
  <thead>
    <tr>
      <th>Caja</th>
      <th>Concepto</th>
      <th>Ingreso</th>
      <th>Egreso</th>
      <th>Saldo</th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @mov_cajas.each do |mov_caja| %>
      <tr>
        <td><%= mov_caja.caja_id %></td>
        <td><%= mov_caja.concepto %></td>
        <td><%= mov_caja.ingreso %></td>
        <td><%= mov_caja.egreso %></td>
        <td><%= mov_caja.saldo %></td>
        <td><%= link_to 'Show', mov_caja %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Mov caja', new_mov_caja_path %>
